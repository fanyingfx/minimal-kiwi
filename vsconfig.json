{
  "textMateRules": [
    { "name": "comment",
      "scope": [ "comment", "comment.line.double-slash.odin", "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#5ed464b8"
      }
    },
    {
      "name": "function",
      "scope": [ "entity.name.function" ],
      "settings": {
        "foreground": "#ECFFFF"
        // "fontStyle": "italic"
      }

    },
    {
      "name": "operator",
      "scope": [ "meta.block" ],
      "settings": {
        "foreground": "#89DDFF",
      }
    },
    {
      "name": "type",
      "scope": [ "storage.type" ],
      "settings": {
        "foreground": "#FFCB6B",
        // "fontStyle": "bold"
      }
    },
    {
      "name": "builtin",
      "scope": [ "support.function" ],
      "settings": {
        "foreground": "#89DDFF",
        // "fontStyle": "bold"
      }
    },
    {
      "name": "keywords",
      "scope": [ "keyword.other.type" ],
      "settings": {
        "foreground": "#89DDFF",
      }
    },
    {
      "name": "empty",
      "scope": [ "meta.block" ],
      "settings": {
        "foreground": "#EEE",
      }
    }
  ]
},
},
